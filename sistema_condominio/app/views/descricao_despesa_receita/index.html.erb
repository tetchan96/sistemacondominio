<p id="notice"><%= notice %></p>

<h1>Descricao Despesa Receita</h1>

<table>
  <thead>
    <tr>
      <th>Descricao</th>
      <th>Natureza</th>
      <th>Valor fixo</th>
      <th>Ativo</th>
      <th>Data inclusao</th>
      <th>Data alteracao</th>
      <th>Apartamento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @descricao_despesa_receita.each do |descricao_despesa_receitum| %>
      <tr>
        <td><%= descricao_despesa_receitum.descricao %></td>
        <td><%= descricao_despesa_receitum.natureza %></td>
        <td><%= descricao_despesa_receitum.valor_fixo %></td>
        <td><%= descricao_despesa_receitum.ativo %></td>
        <td><%= descricao_despesa_receitum.data_inclusao %></td>
        <td><%= descricao_despesa_receitum.data_alteracao %></td>
        <td><%= descricao_despesa_receitum.apartamento %></td>
        <td><%= link_to 'Show', descricao_despesa_receitum %></td>
        <td><%= link_to 'Edit', edit_descricao_despesa_receitum_path(descricao_despesa_receitum) %></td>
        <td><%= link_to 'Destroy', descricao_despesa_receitum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


<p id="notice"><%= notice %></p>

<h1>Receita A Recebers</h1>

<table>
  <thead>
    <tr>
      <th>Valor</th>
      <th>Data</th>
      <th>Natureza</th>
      <th>Ativo</th>
      <th>Data inclusao</th>
      <th>Data alteracao</th>
      <th>Apartamento</th>
      <th>Descricao despesa receita</th>
      <th>Morador</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @receita_a_recebers.each do |receita_a_receber| %>
      <tr>
        <td><%= receita_a_receber.valor %></td>
        <td><%= receita_a_receber.data %></td>
        <td><%= receita_a_receber.natureza %></td>
        <td><%= receita_a_receber.ativo %></td>
        <td><%= receita_a_receber.data_inclusao %></td>
        <td><%= receita_a_receber.data_alteracao %></td>
        <td><%= receita_a_receber.apartamento %></td>
        <td><%= receita_a_receber.descricao_despesa_receita %></td>
        <td><%= receita_a_receber.morador %></td>
        <td><%= link_to 'Show', receita_a_receber %></td>
        <td><%= link_to 'Edit', edit_receita_a_receber_path(receita_a_receber) %></td>
        <td><%= link_to 'Destroy', receita_a_receber, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Receita A Receber', new_receita_a_receber_path %>

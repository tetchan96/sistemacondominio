<div class="breadcrumbs">
  <div class="col-sm-4">
    <div class="page-header float-left">
      <div class="page-title">
        <h1> Usuário</h1>
      </div>
    </div>
  </div>
  <div class="col-sm-8">
    <div class="page-header float-right">
      <div class="page-title">
        <ol class="breadcrumb text-right">
          <li><a href="#">Dashboard</a></li>
          <li><a href="#">Usuário</a></li>
          <li class="active">ID</li>
        </ol>
      </div>
    </div>
  </div>
</div>
  
<div class="content mt-3">
  <div class="animated fadeIn">
    <div class="row">
      <div class="col-lg-12">
        <div class="card">
          <div class="card-header">
            <strong>Usuário: ID</strong>
          </div>
          <div class="card-body card-block">

<p>
  <strong>Nome:</strong>
  <%= @usuario.nome %>
</p>

<p>
  <strong>Login:</strong>
  <%= @usuario.login %>
</p>

<p>
  <strong>Senha:</strong>
  <%= @usuario.senha %>
</p>

<p>
  <strong>Ativo:</strong>
  <%= @usuario.ativo %>
</p>

<p>
  <strong>Data inclusao:</strong>
  <%= @usuario.data_inclusao %>
</p>

<p>
  <strong>Data alteracao:</strong>
  <%= @usuario.data_alteracao %>
</p>

<p>
  <strong>Condominio:</strong>
  <%= @usuario.condominio %>
</p>
</div>
        </div>
        </div>
        </div>
        <%= link_to 'Editar', edit_usuario_path(@usuario), class: "btn btn-outline-primary", role:"button" %>
        <%= link_to 'Voltar', usuarios_path, class: "btn btn-outline-primary", role:"button" %>
      </div>
    </div>
  </div><!-- .animated -->
</div><!-- .content -->

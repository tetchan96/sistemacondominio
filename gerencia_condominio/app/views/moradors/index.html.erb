<div class="breadcrumbs">
  <div class="col-sm-4">
    <div class="page-header float-left">
      <div class="page-title">
        <h1>Morador</h1>
      </div>
    </div>
  </div>
  <div class="col-sm-8">
    <div class="page-header float-right">
      <div class="page-title">
        <ol class="breadcrumb text-right">
          <li><a href="#">Dashboard</a></li>
          <li class="active">Morador</li>
        </ol>
      </div>
    </div>
  </div>
</div>
  
<div class="content mt-3">
  <div class="animated fadeIn">
    <div class="row">

      <p id="notice"><%= notice %></p>
      <!--<h1>Descricao Tipos</h1>-->

      <div class="col-md-12">
        <div class="card">
          <div class="card-header">
            <strong class="card-title">Moradores</strong>
          </div>
          <div class="card-body">
            <table id="bootstrap-data-table" class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Rg</th>
      <th>Cpf</th>
      <th>Tel residencial</th>
      <th>Tel celular</th>
      <th>Tel comercial</th>
      <th>Ramal</th>
      <th>Email</th>
      <th>Parentesco</th>
      <th>Tipo</th>
      <th>Ativo</th>
      <th>Data inclusao</th>
      <th>Data alteracao</th>
      <th>Apartamento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @moradors.each do |morador| %>
      <tr>
        <td><%= morador.nome %></td>
        <td><%= morador.rg %></td>
        <td><%= morador.cpf %></td>
        <td><%= morador.tel_residencial %></td>
        <td><%= morador.tel_celular %></td>
        <td><%= morador.tel_comercial %></td>
        <td><%= morador.ramal %></td>
        <td><%= morador.email %></td>
        <td><%= morador.parentesco %></td>
        <td><%= morador.tipo %></td>
        <td><%= morador.ativo %></td>
        <td><%= morador.data_inclusao %></td>
        <td><%= morador.data_alteracao %></td>
        <td><%= morador.apartamento %></td>
        <td><%= link_to 'Show', morador %></td>
        <td><%= link_to 'Edit', edit_morador_path(morador) %></td>
        <td><%= link_to 'Destroy', morador, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
            </table>
            <%= link_to 'Adicionar Morador', new_morador_path, class: "btn btn-outline-primary", role:"button" %>
          </div>
        </div>
      </div>
    </div>
  </div><!-- .animated -->
</div><!-- .content -->
